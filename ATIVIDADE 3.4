#include <stdio.h>

int main()
{
  int num1;
  int num2;

  printf("Digite o primeiro número: ");
  scanf("%d", &num1);
  printf("Digite o segundo número: ");
  scanf("%d", &num2);
  
  if (num1 > num2){
    printf("O primeiro número é maior que o segundo!");
  }
  else {
    printf("O segundo número é meior que o primeiro!");
  }
  return 0;
  

}
